  "repository": {
    "type": "git",
    "url": "git+https://github.com/autopulated/fastify-dynamodm.git"
  },
  "bugs": {
    "url": "https://github.com/autopulated/fastify-dynamodm/issues"
  },
  "homepage": "https://github.com/autopulated/fastify-dynamodm",
  "author": "James Crosby <3456211+autopulated@users.noreply.github.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.552.0",
    "@aws-sdk/lib-dynamodb": "^3.552.0",
    "aws-sdk-client-mock": "^4.0.0",
    "eslint": "^8.57.0",
    "fastify": "^4.26.2",
    "standard": "^17.1.0",
    "tap": "^18.7.2"
  },
  "dependencies": {
    "dynamodm": "^1.3.2",
    "fastify-plugin": "^4.5.1"
  }
